<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lz.userprovider.user.mapper.UserMapper">
    <insert id="userInsert" parameterType="com.lz.rhapi.user.entity.User">
        insert into t_user(NAME,EMAIL,PASSWORD) values (#{name},#{email},#{password})
    </insert>
    <delete id="userDel">
        delete from t_user where u_id = {uId}
    </delete>
    <select id="userQuery" resultType="java.lang.String">
        select * from t_user where uid = {u_id}
    </select>

    <update id="userUpdate">

    </update>
</mapper>
